class Point {

    private float x;
    private float y;

    Point() {
    }

    Point(float x, float y) {
        this.x=x;
        this.y=y;
    }

    void setX(float x) {
        this.x=x;
    }

    float getX() {
        return x;
    }

    void setY(float y) {
        this.y=y;
    }
    float getY() {
        return y;
    }
}